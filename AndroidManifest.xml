<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="no.minimon.eyecatch"
    android:versionCode="1"
    android:versionName="0.1" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="EyeCatchActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="CreateUserActivity" >
        </activity>
        <activity
            android:name=".game.EyeCatchGameActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:parentActivityName="no.minimon.eyecatch.EyeCatchActivity"
            android:screenOrientation="landscape"
            android:theme="@style/FullscreenTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="no.minimon.eyecatch.EyeCatchActivity" />
        </activity>
        <activity
            android:name=".game.TouchTrainingActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:parentActivityName="no.minimon.eyecatch.EyeCatchActivity"
            android:screenOrientation="landscape"
            android:theme="@style/FullscreenTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="no.minimon.eyecatch.EyeCatchActivity" />
        </activity>
        <activity
            android:name="VideoViewActivity"
            android:screenOrientation="landscape"
            android:theme="@style/FullscreenBlackBackground" >
        </activity>
        <activity
            android:name=".game.ExtraTestingActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:parentActivityName="no.minimon.eyecatch.EyeCatchActivity"
            android:screenOrientation="landscape"
            android:theme="@style/FullscreenTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="no.minimon.eyecatch.EyeCatchActivity" />
        </activity>
        <activity
            android:name=".game.GeneralizationTestingActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:parentActivityName="no.minimon.eyecatch.EyeCatchActivity"
            android:screenOrientation="landscape"
            android:theme="@style/FullscreenTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="no.minimon.eyecatch.EyeCatchActivity" />
        </activity>
        <activity android:name="SelectVideoActivity" >
        </activity>
        <activity android:name="SelectUserAcitivity" >
        </activity>
        <activity android:name="SelectStatisticAcitivity" >
        </activity>
    </application>

</manifest>